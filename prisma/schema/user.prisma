model User {
    id                   Int       @id @default(autoincrement())
    email                String    @unique
    password             String?
    companyName          String?
    googleId             String?   @unique
    phoneNo              String?
    refreshToken         String?
    resetPasswordToken   String?
    resetPasswordExpires DateTime?
    createdAt            DateTime  @default(now())
    events               Event[]
}
