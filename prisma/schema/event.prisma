model Event {
    id                 Int                 @id @default(autoincrement())
    eventName          String
    isPaid             Boolean
    image              String
    address            String
    city               String
    eventDate          DateTime
    userJourney        String[]
    eventTemplate      Json
    attendieType       String[]
    createdAt          DateTime            @default(now())
    adminId            Int // Foreign key field
    admin              Admin               @relation(fields: [adminId], references: [id]) // Define the relation
    eventRegistrations EventRegistration[]
    userJournerys      UserJourney[]
}
